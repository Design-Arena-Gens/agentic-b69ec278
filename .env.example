# Gemini API Key (for AI Assistant features)
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Google Maps API Key (for location features)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Cloud Backend Configuration (Optional - for data sync)
# Choose one: Firebase, AWS Amplify, or Google Cloud

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# AWS Amplify Configuration
NEXT_PUBLIC_AWS_REGION=us-east-1
NEXT_PUBLIC_AWS_USER_POOL_ID=your_user_pool_id
NEXT_PUBLIC_AWS_USER_POOL_CLIENT_ID=your_client_id

# Google Cloud Configuration
NEXT_PUBLIC_GCP_PROJECT_ID=your_gcp_project_id
NEXT_PUBLIC_GCP_API_KEY=your_gcp_api_key
